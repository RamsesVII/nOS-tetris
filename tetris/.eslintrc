{
  "extends": [
    "plugin:react/recommended",
    "eslint:recommended",
    "prettier",
    "prettier/flowtype",
    "prettier/react",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jest/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "es6": true,
    "jasmine": true,
    "browser": true
  },
  "globals": {
    "module": true,
    "require": true,
    "process": true,
    "global": true
  },
  "plugins": ["flowtype", "sort-imports-es6-autofix", "react-native"],
  "rules": {
    "jest/no-disabled-tests": 0,
    "import/no-named-as-default": 0,
    "sort-imports-es6-autofix/sort-imports-es6": [
      2,
      {
        "ignoreCase": false,
        "ignoreMemberSort": false,
        "memberSyntaxSortOrder": ["none", "all", "multiple", "single"]
      }
    ],
    "no-debugger": 0,
    "eqeqeq": [2, "smart"],
    "no-console": 1,
    "no-await-in-loop": 1,
    "no-unused-expressions": [
      "warn",
      {
        "allowShortCircuit": true,
        "allowTernary": true,
        "allowTaggedTemplates": true
      }
    ],
    "no-unused-labels": "warn",
    "no-unused-vars": [
      "warn",
      {
        "args": "none",
        "ignoreRestSiblings": true
      }
    ],
    "no-use-before-define": [
      "warn",
      {
        "functions": false,
        "classes": false,
        "variables": false
      }
    ],
    "no-useless-computed-key": "warn",
    "no-useless-concat": "warn",
    "no-useless-constructor": "warn",
    "no-useless-escape": "warn",
    "no-useless-rename": [
      "warn",
      {
        "ignoreDestructuring": false,
        "ignoreImport": false,
        "ignoreExport": false
      }
    ],
    "no-constant-condition": 1,
    "react/prop-types": 1,
    "react/display-name": 0,
    "react/jsx-no-comment-textnodes": "warn",
    "react/jsx-no-duplicate-props": [
      "warn",
      {
        "ignoreCase": true
      }
    ],
    "react/jsx-no-target-blank": "warn",
    "react/jsx-no-undef": "error",
    "react/jsx-pascal-case": [
      "warn",
      {
        "allowAllCaps": true,
        "ignore": []
      }
    ],
    "react/jsx-uses-react": "warn",
    "react/jsx-uses-vars": "warn",
    "react/no-danger-with-children": "warn",
    "react/no-deprecated": "warn",
    "react/no-direct-mutation-state": "warn",
    "react/no-is-mounted": "warn",
    "react/react-in-jsx-scope": "error",
    "react/require-render-return": "error",
    "react/style-prop-object": "warn",
    "flowtype/boolean-style": [1, "boolean"],
    "flowtype/define-flow-type": 1,
    "flowtype/no-primitive-constructor-types": 1,
    "flowtype/no-types-missing-file-annotation": 1,
    "flowtype/require-parameter-type": 0,
    "flowtype/require-valid-file-annotation": 1,
    "flowtype/use-flow-type": 1,
    "flowtype/valid-syntax": 1
  }
}
